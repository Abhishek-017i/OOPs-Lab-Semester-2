class Date {
    private int month,day,year;
    Date(int month, int day, int year) {
        this.month = month;
        this.day = day;
        this.year = year;
    }
                                                        //set
    public void setMonth(int month) {
        this.month = month;
    }
    public void setDay(int day) {
        this.day = day;
    }
    public void setYear(int year) {
        this.year = year;
    }
                                                        //get
    public int getMonth() {
        return month;
    }
    public int getDay() {
        return day;
    }
    public int getYear() {
        return year;
    }

    public void displayDate() {
        System.out.printf("%d/%d/%d \n", day, month, year);
    }
}

public class DateTest {
    public static void main(String[] args) {
        Date myDate = new Date(01, 01, 2026);

        System.out.print("Initial date: ");
        myDate.displayDate();    //Get is used to fetch values from private.

        myDate.setMonth(5);      //Set is used here to update the date.
        myDate.setDay(10);
        myDate.setYear(2026);

        System.out.print("\nFinal date after using set: ");
        myDate.displayDate();
    }
